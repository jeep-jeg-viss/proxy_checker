APP_NAME=Proxy Checker API
APP_VERSION=0.3.0
API_PREFIX=/api
CORS_ALLOW_ORIGINS=["http://localhost:3000"]

# Supabase Postgres connection string (async SQLAlchemy + asyncpg).
# Replace placeholders with your values.
DATABASE_URL=postgresql+asyncpg://postgres:[YOUR_DB_PASSWORD]@db.[YOUR_PROJECT_REF].supabase.co:5432/postgres?ssl=require

# Auth0 API protection settings
AUTH0_DOMAIN=your-tenant.us.auth0.com
AUTH0_AUDIENCE=https://proxy-checker-api
AUTH0_ISSUER=https://your-tenant.us.auth0.com/
AUTH0_ALGORITHMS=["RS256"]
PROXY_PASSWORD_SECRET=replace_with_a_long_random_secret

# Optional toggles
DB_ECHO=false
DB_AUTO_CREATE=true
DB_STATEMENT_CACHE_SIZE=0
